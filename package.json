{
  "name": "jahlgren.eu",
  "version": "1.0.0",
  "description": "Description",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "css": "npm-run-all --sequential css-compile css-prefix css-minify",
    "css-compile": "node-sass --include-path node_modules/ --style nested resources/scss/jahlgren.scss data/build/css/jahlgren.css",
    "css-prefix": "postcss --use autoprefixer --output data/build/css/jahlgren.prefix.css data/build/css/jahlgren.css",
    "css-minify": "uglifycss data/build/css/jahlgren.prefix.css > public/css/jahlgren.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jailgreen/jahlgren.eu.git"
  },
  "keywords": [],
  "author": "jailgreen <jukka@jahlgren.eu>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jailgreen/jahlgren.eu/issues"
  },
  "homepage": "https://github.com/jailgreen/jahlgren.eu#readme",
  "dependencies": {
    "autoprefixer": "^8.4.1",
    "bootstrap": "^4.1.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.14.3",
    "postcss-cli": "^5.0.0",
    "stylelint": "^9.2.0",
    "uglify-js": "^3.3.23",
    "uglifycss": "0.0.29"
  },
  "devDependencies": {}
}
